{
  "name": "houston-api",
  "version": "0.0.2",
  "description": "Source of truth for the Astronomer Platform",
  "main": "index.js",
  "bin": {
    "houston-api": "index.js"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "engineStrict": true,
  "scripts": {
    "start": "node --use-strict index.js",
    "dev": "./node_modules/.bin/nodemon --use-strict --watch src",
    "create-migration": "./node_modules/.bin/knex migrate:make",
    "test": "sh test.sh",
    "test-db": "docker-compose -p houston-api-test -f test-docker-compose.yaml up -d",
    "test-db-down": "docker-compose -p houston-api-test -f test-docker-compose.yaml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astronomerio/houston-api.git"
  },
  "author": "Astronomer <humans@astronomer.io>",
  "license": "Astronomer EE",
  "private": true,
  "bugs": {
    "url": "https://github.com/astronomerio/houston-api/issues"
  },
  "homepage": "https://github.com/astronomerio/houston-api#readme",
  "dependencies": {
    "@moilandtoil/sealab-application": "0.0.8",
    "@moilandtoil/sealab-operation": "0.0.5",
    "@moilandtoil/sealab-schema-builder": "0.0.6",
    "@moilandtoil/sealab-type": "0.0.5",
    "apollo-cache-control": "^0.1.0",
    "apollo-errors": "^1.9.0",
    "apollo-server-core": "^1.3.4",
    "apollo-upload-server": "^5.0.0",
    "aws-lambda": "^0.1.2",
    "bcrypt": "^1.0.3",
    "commander": "github:astronomerio/commander#helm",
    "generate-password": "^1.4.0",
    "googleapis": "^32.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.21.0",
    "graphql-yoga": "^1.3.2",
    "grpc": "^1.9.1",
    "grpc-caller": "^0.4.0",
    "haikunator": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.14.3",
    "lodash": "^4.17.5",
    "node-cache": "^4.2.0",
    "node-forge": "^0.7.5",
    "objection": "^1.0.0",
    "objection-password": "^0.2.0",
    "objection-soft-delete": "^1.0.4",
    "pass": "^0.2.0",
    "pg": "^7.4.1",
    "request-promise-native": "^1.0.5",
    "semver": "^5.5.0",
    "shortid": "^2.2.8",
    "uuid": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "jest-cli": "^22.3.0",
    "knex-cleaner": "^1.1.4",
    "nodemon": "^1.15.0"
  }
}
